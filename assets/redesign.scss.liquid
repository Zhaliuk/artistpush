.template-cart {
  position: relative;

  &:after {
    content: '';
    display: block;
    position: absolute;
    z-index: -1;
    top: 0;
    left: 25%;
    width: 891.42px;
    height: 1934.82px;
    background: #000000;
    filter: blur(189px);
    transform: translateY(-25%) rotate(120deg);
    border-radius: 50%;
  }

  &:before {
    content: '';
    display: block;
    position: absolute;
    z-index: -1;
    top: 500px;
    left: 50%;
    width: 570.79px;
    height: 1238.89px;
    background: #F0502C;
    filter: blur(189px);
    transform: translateX(-50%) rotate(-59.33deg);
    border-radius: 50%;
  }
}

.up {
  display: none;
}

.alert {
  display: flex;
  align-items: center;
  align-content: center;
  justify-content: space-between;
  padding: 15px;
  background: #F0502C;
  color: #fff;
  font-size: 20px;
  line-height: 24px;
  letter-spacing: 0.04em;
  margin-bottom: 40px;

  a {
    text-decoration: underline;
  }

  .alert__close {
    display: block;
    position: relative;
    width: 24px;
    height: 24px;
    cursor: pointer;

    &:before,
    &:after {
      content: '';
      display: block;
      position: absolute;
      top: 50%;
      left: 0;
      width: 100%;
      height: 2px;
      background-color: #fff;
    }

    &:before {
      transform: translateY(-1px) rotate(-45deg);
    }

    &:after {
      transform: translateY(-1px) rotate(45deg);
    }
  }
}

