}

.template-index .header__top:before {
  display: block;
  position: absolute;
  z-index: -1;
  top: -220px;
  right: 0;
  width: 458px;
  height: 994px;
  background: #AD131C;
  filter: blur(120px);
  transform: rotate(120deg);
}

.template-index:before {
  content: '';
  display: block;
  position: absolute;
  z-index: -1;
  top: -220px;
  right: 0;
  width: 458px;
  height: 994px;
  background: #AD131C;
  filter: blur(120px);
  transform: rotate(120deg);
}

.template-list-collections {

  .products-grid:before {
    content: '';
    display: block;
    position: absolute;
    top: 50%;
    left: 50%;
    width: 830px;
    height: 1800px;
    border-radius: 50%;
    background: #000000;
    filter: blur(189px);
    transform: translate(-50%, -50%) rotate(-75deg);
  }
}

.template-collection,
.template-list-collections {
  position: relative;

  &:after {
    content: '';
    display: block;
    position: absolute;
    z-index: -2;
    top: -700px;
    right: 0;
    width: 891.42px;
    height: 1934.82px;
    background: #000000;
    filter: blur(189px);
    transform: rotate(120deg);
    border-radius: 50%;
  }

  &:before {
    content: '';
    display: block;
    position: absolute;
    z-index: -1;
    right: 200px;
    bottom: -50px;
    width: 360px;
    height: 788px;
    background: #F0502C;
    filter: blur(189px);
    transform: rotate(-45deg);
    border-radius: 50%;
  }
}

.header__top {
  position: relative;
  z-index: 2;
  padding: 35px 0;
  border-bottom: 1px solid #2B2B2B;

  > * {
    position: relative;
    z-index: 2;
  }
}

.header__nav-toggle,
.header__nav-mob {
  display: none;
  cursor: pointer;
}

.header__nav {
  position: relative;
  z-index: 2;
  transform: translateY(-1px);

  ul {
    li {
      margin-right: 25px;
      font-size: 15px;
      font-weight: 600;
      line-height: 16px;
      letter-spacing: 0.105em;
      text-transform: uppercase;
      transition: .3s ease-in-out;
      padding: 15px 0 60px;

      &.has-child {

        &:hover a:before {
          transform: translateY(-50%) rotate(-45deg);
          background-color: rgba(255, 255, 255, 1);
        }

        &:hover a:after {
          transform: translateY(-50%) rotate(45deg);
          background-color: rgba(255, 255, 255, 1);
        }

        > a {
          position: relative;
          padding-right: 30px;

          &:before,
          &:after {
            content: '';
            display: block;
            position: absolute;
            top: 50%;
            right: 15px;
            width: 2px;
            height: 8px;
            background-color: #838382;
            transition: .3s;
          }

          &:before {
            transform: translateY(-50%) rotate(45deg);
            right: 15px;
          }

          &:after {
            transform: translateY(-50%) rotate(-45deg);
            right: 20px;
          }
        }

        &:hover .sub-menu {
          display: block;
        }

        .sub-menu {
          display: none;
          position: absolute;
          z-index: 5;
          top: 100%;
          left: 50%;
          transform: translateX(-50%);
          width: 100vw;
          background: #1E1D1C;
          padding: 40px 0;

          .grid {
            display: grid;
            grid-column-gap: 60px;
            grid-template-columns: repeat(4, 1fr);
          }

          .grid__item {
            position: relative;
            padding-top: 60px;

            h5 {
              position: absolute;
              top: 0;
              left: 0;
              margin: 0;
              margin-bottom: 30px;
              font-weight: 500;
              font-size: 24px;
              line-height: 28px;
              letter-spacing: 0.02em;
              text-transform: none;
            }
          }

          li {
            margin-right: 0;
            padding: 0;
            margin-bottom: 20px;

            &:last-child {
              margin-bottom: 0;
            }

            a {
              font-weight: normal;
              font-size: 17px;
              line-height: 21px;
              letter-spacing: 0.11em;
              text-transform: none;
            }
          }
        }
      }

      &:last-child {
        margin: 0;
      }

      &.site-nav--active {
        border-top: 1px solid #AD131C;
      }

      &.active,
      &:hover {
        > a {
          color: rgba(#fff, 1);
        }
      }

      > a {
        transition: .3s;
        color: rgba(#fff, .5);
      }
    }
  }
}

