}

.popular-products-layout {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  align-content: flex-start;
  position: relative;
  justify-content: space-between;
  padding: 30px;
  background: rgba(#F0502C, .1);
  transition: .3s ease-in-out;

  .buy {
    [type="number"] {
      display: none;
    }

    .buy-btn {
      display: flex;
      align-content: center;
      align-items: center;
      justify-content: flex-end;
      font-weight: bold;
      font-size: 18px;
      line-height: 23px;
      text-align: center;
      letter-spacing: 0.08em;
      text-transform: uppercase;
      color: #E34429;
      background-color: transparent;
      padding: 0;
      border: 0;
      cursor: pointer;

      span {
        width: 0;
        overflow: hidden;
        transform-origin: right center ;
        transition: .3s;
        white-space: nowrap;
      }

      i {
        margin-right: 10px;
        font-size: 30px;
      }
    }
  }

  &:hover {
    background: rgba(#AD131C, 1);

    .pct {
      opacity: 1;
    }

    .buy .buy-btn span {
      width: 140px;
    }
  }

  .wc_avg_bracket {
    display: none;
  }

  .wc_product_review_avg_badge_count {
    font-size: 14px;
    line-height: 18px;
    letter-spacing: 0.11em;
  }

  .wc_product_review_text {
    font-size: 14px;
    line-height: 18px;
    letter-spacing: 0.11em;
    opacity: 0.3;
  }

  .wc_review_badge_star {
    display: flex;
    align-items: flex-start;
    align-content: flex-start;
    justify-content: flex-start;
  }

  .wc_icon_color {
    display: block;
    margin-right: 3px;

    &:last-child {
      margin-right: 0;
    }

    svg {
      display: block;
      height: 12px;
      width: 13px !important;
    }
  }

  .wc_product_review_title {
    position: relative;

    .wc_product_review_text {
      position: absolute;
      top: calc(100% + 6px);
      right: 0;
      white-space: nowrap;
    }
  }

  .heading {
    width: 100%;
    margin-bottom: 95px;
  }

  .pct {
    opacity: .6;
    transition: .3s ease-in-out;
  }

  .data {
    width: 100%;
    margin-top: auto;
    flex-wrap: nowrap;
  }

  .info {
    width: calc(100% - 130px);
  }

  .title {
    font-weight: 500;
    font-size: 24px;
    line-height: 28px;
    letter-spacing: .02em;
  }

  .price {
    flex-direction: column;
    width: 90px;

    .current-price {
      color: #F0502C;
      font-size: 36px;
      font-weight: bold;
      line-height: 42px;
    }

    .old-price {
      opacity: 0.3;
      font-size: 18px;
      line-height: 23px;
      letter-spacing: 0.11em;
      text-decoration: line-through;
    }
  }
}

