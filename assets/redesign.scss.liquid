/* Read more*/
.read-more-text_section{
  .content.read_mode_title{
    margin: 0 auto;
    width: auto;
  }
  .read-more-btn{
  touch-action: manipulation;
  }
  .read-more-content{
    overflow: hidden;
    width: 100%;
    &.active{
      display: block;
    }
    ul{
      padding-inline-start: 40px;
      list-style: unset;
      li{
        list-style: unset;
      }
      li::marker{
        color:#f0502c;
      }
    }
    ol{
      padding-inline-start: 40px;
      list-style: unset;
      li{
        list-style: unset;
      }
      li::marker{
        color:#f0502c;
      }
    }
    .content{
    column-count: unset;
    column-gap: unset;
    margin: 0 auto;
    display: none;
    &.active{
      display: block;
    }
  }

    .read-more-content_block{
      margin-bottom: 30px;
      .block_title{
        font-size: 40px;
        font-weight: bold;
        line-height: 40px;
      }
      .block_text{
        margin-bottom: 50px;
      }
      .block_list{
        display: flex;
        justify-content: space-between;
        .block_list-item{
          width: calc(100%/3 - 30px);
          .item-img{
            display: block;
            max-width: 65px;
            height: 65px;
            margin: 0 auto 15px auto;
          }
          .item-title{
            font-size: 24px;
            font-weight: bold;
            margin: 0;
            margin-bottom: 15px;
          }
          .item-text{

          }
        }
      }
    }
  }
}

